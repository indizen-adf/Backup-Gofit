{
	"name": "DS_CSV_DataLake",
	"properties": {
		"description": "Dataset gen√©rico para leer ficheros CSV del Data Lake (separados por coma y con cabecera)",
		"linkedServiceName": {
			"referenceName": "DataLakeIngesport_ADLS_G2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"container": {
				"type": "string"
			},
			"domain": {
				"type": "string"
			},
			"object": {
				"type": "string"
			},
			"file": {
				"type": "string"
			}
		},
		"folder": {
			"name": "DataLakeStorage"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(dataset().file, '.csv')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat(dataset().domain, '/', dataset().object)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}